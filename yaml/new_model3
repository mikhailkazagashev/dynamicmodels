- name: data
  title: Пользователи 
  fields:
  - {id: name, title: Имя, type: char}
  - {id: date_joined, title: Дата регистрациии, type: date}

- name: data1
  title: Комнаты 
  fields:
  - {id: department, title: Отдел, type: char}
  - {id: spots, title: Вместимость, type: int}

- name: data2
  title: Работники 
  fields:
  - {id: name, title: Имя, type: char}
  - {id: paycheck, title: Зарплата, type: int}
  - {id: date_joined, title: Дата поступления
      на работу, type: date}